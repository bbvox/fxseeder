{
  "name": "fxseeder",
  "version": "0.1.0",
  "description": "Seed data from sites",
  "main": "index.js",
  "scripts": {
    "start:mongo": "echo \"{}\" > $npm_package_config_mongodb && node ./commands/mongodb.js",
    "start": "./index.js",
    "debug": "./index.js --debug",
    "test": "mocha --recursive --timeout 5000 --exit",
    "test:cover": "nyc --reporter=lcov ./node_modules/mocha/bin/mocha  --recursive --timeout 5000 --exit",
    "test:lcov": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:cover-html": "nyc --reporter=html ./node_modules/mocha/bin/mocha  --recursive --timeout 5000 --exit",
    "OK": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.5.5",
    "mongoose": "^5.7.1",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "mongodb-memory-server": "^5.2.3",
    "nock": "^11.3.4",
    "sinon": "^7.4.2"
  },
  "config": {
    "mongodb": "./config/mongo.json"
  },
  "bin": {
    "start_mongodb": "./commands/mongodb.js"
  }
}
